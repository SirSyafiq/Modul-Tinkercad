int counter;


void setup()
{
pinMode(2, OUTPUT); 
  pinMode(3, OUTPUT); 
  pinMode(4, OUTPUT); 
  pinMode(5, OUTPUT); 
  pinMode(6, OUTPUT); 
  pinMode(7, OUTPUT); 
  pinMode(8, OUTPUT); 
  pinMode(9, OUTPUT); 
  pinMode(10, OUTPUT);
}


void loop()
 
{
digitalWrite (2, HIGH);
digitalWrite (5, HIGH);
digitalWrite (8, HIGH);
delay (5000);
for (counter = 0; counter <5; ++counter)
{
digitalWrite (8, HIGH);
delay (400);
digitalWrite (8, LOW);
delay (400);
}
digitalWrite (9, HIGH);
delay (3000);
digitalWrite (9, LOW);
digitalWrite (10, HIGH);
delay (2000);
digitalWrite (2, LOW);
digitalWrite (4, HIGH);
delay (5000);
for (counter = 0; counter <5; ++counter)
{
digitalWrite (4, HIGH);
delay (400);
digitalWrite (4, LOW);
delay (400);
}
digitalWrite (3, HIGH);
delay (3000);
digitalWrite (3, LOW);
digitalWrite (2, HIGH);
delay (2000);
digitalWrite (5, LOW);
digitalWrite (7, HIGH);
delay (5000);
for (counter = 0; counter <5; ++counter)
{
digitalWrite (7, HIGH);
delay (400);
digitalWrite (7, LOW);
delay (400);
}
digitalWrite (6, HIGH);
delay (3000);
digitalWrite (6, LOW);
digitalWrite (5, HIGH);
delay (2000);
digitalWrite (10, LOW);
digitalWrite (8, HIGH);
delay (5000);

}
